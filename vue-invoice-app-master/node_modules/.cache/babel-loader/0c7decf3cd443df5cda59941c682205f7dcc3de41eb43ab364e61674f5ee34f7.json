{"ast":null,"code":"import Invoice from \"../components/Invoice\";\nimport { mapMutations, mapState } from \"vuex\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      filterMenu: null,\n      filteredInvoice: null\n    };\n  },\n  components: {\n    Invoice\n  },\n  methods: {\n    ...mapMutations([\"TOGGLE_INVOICE\"]),\n    newInvoice() {\n      this.TOGGLE_INVOICE();\n    },\n    toggleFilterMenu() {\n      this.filterMenu = !this.filterMenu;\n    },\n    filteredInvoices(e) {\n      if (e.target.innerText === \"Clear Filter\") {\n        this.filteredInvoice = null;\n        return;\n      }\n      this.filteredInvoice = e.target.innerText;\n    }\n  },\n  computed: {\n    ...mapState([\"invoiceData\"]),\n    filteredData() {\n      return this.invoiceData.filter(invoice => {\n        if (this.filteredInvoice === \"Draft\") {\n          return invoice.invoiceDraft === true;\n        }\n        if (this.filteredInvoice === \"Pending\") {\n          return invoice.invoicePending === true;\n        }\n        if (this.filteredInvoice === \"Paid\") {\n          return invoice.invoicePaid === true;\n        }\n        return invoice;\n      });\n    }\n  }\n};","map":{"version":3,"names":["Invoice","mapMutations","mapState","name","data","filterMenu","filteredInvoice","components","methods","newInvoice","TOGGLE_INVOICE","toggleFilterMenu","filteredInvoices","e","target","innerText","computed","filteredData","invoiceData","filter","invoice","invoiceDraft","invoicePending","invoicePaid"],"sources":["/Users/jeniferpantoja/Downloads/vue-invoice-app-master/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"home container\">\n    <!-- Header -->\n    <div class=\"header flex\">\n      <div class=\"left flex flex-column\">\n        <h1>Invoices</h1>\n        <span>There are {{ invoiceData.length }} total invoices</span>\n      </div>\n      <div class=\"right flex\">\n        <div @click=\"toggleFilterMenu\" class=\"filter flex\">\n          <span\n            >Filter by status <span v-if=\"filteredInvoice\">: {{ filteredInvoice }}</span></span\n          >\n          <img src=\"@/assets/icon-arrow-down.svg\" alt=\"\" />\n          <ul v-show=\"filterMenu\" class=\"filter-menu\">\n            <li @click=\"filteredInvoices\">Draft</li>\n            <li @click=\"filteredInvoices\">Pending</li>\n            <li @click=\"filteredInvoices\">Paid</li>\n            <li @click=\"filteredInvoices\">Clear Filter</li>\n          </ul>\n        </div>\n        <div @click=\"newInvoice\" class=\"button flex\">\n          <div class=\"inner-button flex\">\n            <img src=\"@/assets/icon-plus.svg\" alt=\"\" />\n          </div>\n          <span>New Invoice</span>\n        </div>\n      </div>\n    </div>\n    <!-- Invoices -->\n    <div v-if=\"invoiceData.length > 0\">\n      <Invoice v-for=\"(invoice, index) in filteredData\" :invoice=\"invoice\" :key=\"index\" />\n    </div>\n    <div v-else class=\"empty flex flex-column\">\n      <img src=\"@/assets/illustration-empty.svg\" alt=\"\" />\n      <h3>There is nothing here</h3>\n      <p>Create a new invoice by clicking the New Invoice button and get started</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Invoice from \"../components/Invoice\";\nimport { mapMutations, mapState } from \"vuex\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      filterMenu: null,\n      filteredInvoice: null,\n    };\n  },\n  components: {\n    Invoice,\n  },\n  methods: {\n    ...mapMutations([\"TOGGLE_INVOICE\"]),\n    newInvoice() {\n      this.TOGGLE_INVOICE();\n    },\n\n    toggleFilterMenu() {\n      this.filterMenu = !this.filterMenu;\n    },\n\n    filteredInvoices(e) {\n      if (e.target.innerText === \"Clear Filter\") {\n        this.filteredInvoice = null;\n        return;\n      }\n      this.filteredInvoice = e.target.innerText;\n    },\n  },\n  computed: {\n    ...mapState([\"invoiceData\"]),\n\n    filteredData() {\n      return this.invoiceData.filter((invoice) => {\n        if (this.filteredInvoice === \"Draft\") {\n          return invoice.invoiceDraft === true;\n        }\n        if (this.filteredInvoice === \"Pending\") {\n          return invoice.invoicePending === true;\n        }\n        if (this.filteredInvoice === \"Paid\") {\n          return invoice.invoicePaid === true;\n        }\n        return invoice;\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.home {\n  color: #fff;\n\n  .header {\n    margin-bottom: 65px;\n\n    .left,\n    .right {\n      flex: 1;\n    }\n\n    .right {\n      justify-content: flex-end;\n      align-items: center;\n\n      .button,\n      .filter {\n        align-items: center;\n\n        span {\n          font-size: 12px;\n        }\n      }\n\n      .filter {\n        position: relative;\n        margin-right: 40px;\n        cursor: pointer;\n\n        img {\n          margin-left: 12px;\n          width: 9px;\n          height: 5px;\n        }\n\n        .filter-menu {\n          width: 120px;\n          position: absolute;\n          top: 25px;\n          list-style: none;\n          background-color: #1e2139;\n          box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);\n\n          li {\n            cursor: pointer;\n            font-size: 12px;\n            padding: 10px 20px;\n\n            &:hover {\n              color: #1e2139;\n              background-color: #fff;\n            }\n          }\n        }\n      }\n\n      .button {\n        padding: 8px 10px;\n        background-color: #7c5dfa;\n        border-radius: 40px;\n\n        .inner-button {\n          margin-right: 8px;\n          border-radius: 50%;\n          padding: 8px;\n          align-items: center;\n          justify-content: center;\n          background-color: #fff;\n          img {\n            width: 10px;\n            height: 10px;\n          }\n        }\n      }\n    }\n  }\n\n  .empty {\n    margin-top: 160p